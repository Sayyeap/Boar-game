{
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1",
            "headers": {
                "Cache-Control": "no-store, no-cache, must-revalidate"
            }
        },
        {
            "src": "/src/(.*)",
            "dest": "/src/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}